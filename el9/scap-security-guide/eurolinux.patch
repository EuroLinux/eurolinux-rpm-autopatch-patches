+  <linux:rpminfo_state id="state_eurolinux9_system" version="1">
+    <linux:version operation="pattern match">^9.*$</linux:version>
+  </linux:rpminfo_state>
+  <linux:rpminfo_object id="obj_eurolinux9_system" version="1">
+    <linux:name>el-release</linux:name>
+  </linux:rpminfo_object>
+
+</def-group>

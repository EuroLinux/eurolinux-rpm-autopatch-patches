+<def-group>
+  <definition class="compliance" id="ensure_eurolinux_gpgkey_installed" version="1">
+    <metadata>
+      <title>EuroLinux gpg-pubkey Package Installed</title>
+      <affected family="unix">
+        <platform>multi_platform_eurolinux</platform>
+      </affected>
+      <description>The EuroLinux key packages are required to be installed.</description>
+    </metadata>
+    <criteria comment="Vendor GPG keys" operator="OR">

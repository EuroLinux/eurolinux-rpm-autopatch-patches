+<def-group>
+  <definition class="inventory"
+  id="installed_OS_is_eurolinux9" version="1">
+    <metadata>
+      <title>EuroLinux 9</title>
+      <affected family="unix">
+        <platform>multi_platform_all</platform>
+      </affected>
+      <reference ref_id="cpe:/o:eurolinux:eurolinux:9"
+      source="CPE" />
+
+      <description>The operating system installed on the system is
+      EuroLinux 9</description>
+    </metadata>
+    <criteria>
+      <extend_definition comment="Installed OS is part of the Unix family"
+      definition_ref="installed_OS_is_part_of_Unix_family" />
+      <criteria operator="OR">
+          <criterion comment="EuroLinux 9 System is installed"
+            test_ref="test_eurolinux9_system" />
+      </criteria>
+    </criteria>
+  </definition>
+
+  <linux:rpminfo_test check="all" check_existence="at_least_one_exists" comment="el-release is version 9" id="test_eurolinux9_system" version="1">
